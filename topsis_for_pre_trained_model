      "source": [
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Define models and criteria\n",
        "models = [\"GPT-4\", \"LLaMA 2\", \"Mistral 7B\", \"Falcon 40B\", \"Claude 2\", \"PaLM 2\", \"Bloom 176B\"]\n",
        "criteria = [\"Perplexity\", \"BLEU\", \"Speed\", \"Size\", \"Context\", \"Fine-tune Diff\"]\n",
        "weights = np.array([0.25, 0.20, 0.15, 0.15, 0.15, 0.10])  # Importance of each criterion\n",
        "beneficial = [False, True, True, False, True, False]  # Higher is better?\n",
        "\n",
        "# Decision matrix (rows: models, columns: criteria)\n",
        "data = np.array([\n",
        "    [12, 0.85, 50, 80, 128000, 3],\n",
        "    [18, 0.75, 80, 65, 8000, 4],\n",
        "    [22, 0.70, 120, 12, 8000, 2],\n",
        "    [20, 0.72, 60, 40, 4000, 3.5],\n",
        "    [15, 0.80, 45, 50, 200000, 3],\n",
        "    [14, 0.78, 55, 70, 32000, 3.5],\n",
        "    [25, 0.65, 30, 176, 2000, 5]\n",
        "])\n",
        "\n",
        "# Normalize the decision matrix\n",
        "norm_data = data / np.sqrt((data ** 2).sum(axis=0))\n",
        "\n",
        "# Weighted normalized decision matrix\n",
        "weighted_data = norm_data * weights\n",
        "\n",
        "# Determine ideal best and worst solutions\n",
        "ideal_best = np.max(weighted_data, axis=0) * np.array(beneficial) + np.min(weighted_data, axis=0) * np.array([not b for b in beneficial])\n",
        "ideal_worst = np.min(weighted_data, axis=0) * np.array(beneficial) + np.max(weighted_data, axis=0) * np.array([not b for b in beneficial])\n",
        "\n",
        "# Compute Euclidean distances to ideal best and worst\n",
        "D_best = np.sqrt(((weighted_data - ideal_best) ** 2).sum(axis=1))\n",
        "D_worst = np.sqrt(((weighted_data - ideal_worst) ** 2).sum(axis=1))\n",
        "\n",
        "# Compute TOPSIS scores\n",
        "scores = D_worst / (D_best + D_worst)\n",
        "\n",
        "# Rank models based on scores\n",
        "ranking = np.argsort(scores)[::-1]\n",
        "ranked_models = [models[i] for i in ranking]\n",
        "ranked_scores = [scores[i] for i in ranking]\n",
        "\n",
        "# Display results in table\n",
        "result_df = pd.DataFrame({\"Model\": ranked_models, \"TOPSIS Score\": ranked_scores})\n",
        "print(result_df)\n",
        "\n",
        "# Plot results\n",
        "def plot_topsis_scores(models, scores):\n",
        "    plt.figure(figsize=(10, 6))\n",
        "    plt.barh(models[::-1], scores[::-1], color='skyblue')\n",
        "    plt.xlabel(\"TOPSIS Score\")\n",
        "    plt.ylabel(\"Models\")\n",
        "    plt.title(\"TOPSIS Score Comparison for Text Generation Models\")\n",
        "    plt.grid(axis='x', linestyle='--', alpha=0.7)\n",
        "    plt.show()\n",
        "\n",
        "plot_topsis_scores(ranked_models, ranked_scores)\n"
      ]
    },
